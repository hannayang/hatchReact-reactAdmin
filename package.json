{
  "name": "hatch-react",
  "version": "2.0.0",
  "description": "Hatch a React project!",
  "homepage": "https://github.com/axiomzen/hatch-react#readme",
  "bugs": {
    "url": "https://github.com/axiomzen/hatch-react/issues"
  },
  "license": "UNLICENSED",
  "author": "Axiom Zen",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axiomzen/hatch-react.git"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development node server.js",
    "build": "cross-env next build",
    "start": "cross-env NODE_ENV=production node server.js",
    "eslint": "eslint src/ stories/ --ext .js,.jsx",
    "stylelint": "stylelint 'src/**/*.css' --config ./stylelint.config.js",
    "test": "TZ=UTC jest --config ./jest.config.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "storybook": "start-storybook -p 9001 -c ./.storybook -s ./",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@axiomzen/cz-adaptor"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run eslint && npm run stylelint && npm run test"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link": "^1.2.12",
    "apollo-link-context": "^1.0.18",
    "apollo-link-error": "^1.1.11",
    "apollo-link-http": "^1.5.15",
    "apollo-link-ws": "^1.0.18",
    "apollo-utilities": "^1.3.2",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.4",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.14",
    "next": "^11.1.0",
    "next-cookies": "^1.1.2",
    "next-images": "^1.1.2",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "ra-data-json-server": "^2.9.7",
    "react": "^16.8.4",
    "react-admin": "^2.9.7",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.8.4",
    "sitemap": "^3.2.2",
    "subscriptions-transport-ws": "^0.9.16",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@axiomzen/cz-adaptor": "^1.3.7",
    "@babel/core": "^7.5.4",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "commitizen": "^4.0.0",
    "csscomb": "^4.3.0",
    "cssnano": "^4.1.10",
    "cypress": "^3.4.1",
    "danger": "^9.1.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-cypress": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0"
  },
  "peerDependencies": {},
  "bundledDependencies": [],
  "optionalDependencies": {},
  "engines": {
    "node": "^10.15",
    "npm": "^6.5"
  },
  "private": true
}
